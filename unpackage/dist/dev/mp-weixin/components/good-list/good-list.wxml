<view class="good-list"><block wx:for="{{list}}" wx:for-item="good" wx:for-index="__i0__" wx:key="id"><view class="good-li" id="{{'good'+good.id}}"><image class="good-img" src="{{good.goodImg}}" mode="widthFix"></image><view class="flex-item"><view class="good-name">{{good.goodName}}</view><text class="good-price">{{good.goodPrice+" 元"}}</text><text class="good-sold">{{"已售"+good.goodSold+"件"}}</text></view></view></block></view>